  </DataSources>
  <DataSets>
    <DataSet Name="DS">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@poup">
            <Value>=Parameters!poup.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@minost">
            <Value>=Parameters!minost.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>;with allsfsosts as
(
	select i.IdSf, i.kpok, i.numsf, i.datpltr, i.sumpltr,
		   sumopl = dbo.uf_GetSfSumOpl(i.idsf, null)
	from SfHeader i
	where i.PayStatus = 1
	and i.poup = @poup
),
allpredosts as
(
select p.idpo, p.kgr, p.ndok, p.datvvod, p.sumpropl,
       ostatok = p.sumpropl - p.sumotgr,
       typedoc = case when p.Direction = 0 then 2 else 3 end
from Predopls p
where p.poup = @poup
and p.sumotgr &lt; p.sumpropl
and p.sumpropl - p.sumotgr &lt;= @minost
)
select a.kpok,
       numdoc = a.numsf,
       datdoc = a.datpltr,
	   ostatok = a.sumpltr - a.sumopl,
	   typedoc = 1
from allsfsosts a
where a.sumpltr - a.sumopl &lt;= @minost

union all

select kpok = p.kgr,
       numdoc = p.ndok,
       datdoc = p.datvvod,
	   ostatok = p.ostatok,
	   typedoc = p.typedoc
from allpredosts p</CommandText>
      </Query>
      <Fields>
        <Field Name="numdoc">
          <DataField>numdoc</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="datdoc">
          <DataField>datdoc</DataField>
          <rd:TypeName>System.DateTime</rd:TypeName>
        </Field>
        <Field Name="ostatok">
          <DataField>ostatok</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="typedoc">
          <DataField>typedoc</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="kpok">
          <DataField>kpok</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="poupdata">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@poup">
            <Value>=Parameters!poup.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>select naipoup
from uv_Moup
where kodpoup = @poup</CommandText>
      </Query>
      <Fields>
        <Field Name="naipoup">
          <DataField>naipoup</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="poups">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandText>select kodpoup, naipoup = '[' + cast(kodpoup as varchar(10)) + '] ' + rtrim(naipoup)
from dbo.uf_GetMyActivePoups()</CommandText>
      </Query>
      <Fields>
        <Field Name="kodpoup">
          <DataField>kodpoup</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="naipoup">
          <DataField>naipoup</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
