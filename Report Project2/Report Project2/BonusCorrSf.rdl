    <DataSet Name="OldRnns">
      <Query>
        <DataSourceName>DS</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@idsf">
            <Value>=Parameters!IdSf.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>;WITH docs AS 
(
	SELECT 
         idolddog = od.iddog,
         t.kpr,
         DocNum = t.DocumentNumber,
         DocDate = t.datgr,
         DocPeriod = dbo.uf_DatMake(t.datgr,2),
         KolProd = SUM(t.kolf),
         OldCenProd = t.CENA,
         KodCenProd = t.KODCEN,
         NdsStake = t.PRODNDS
    FROM dbo.SfProduct g
    INNER JOIN dbo.SfProductOtgr o ON g.idprilsf = o.idprilsf 
    INNER JOIN dbo.arc_p623 t ON o.idp623 = t .idp623 
    inner join dbo.uv_Post od on t.KDOG = od.kdog
    WHERE g.idsf = @idsf
    GROUP BY od.iddog, t.kpr, t.DocumentNumber,
             t.datgr, t.CENA, t.KODCEN, t.PRODNDS    
)
SELECT 
	ProdName = ISNULL(pr.name,''),
	DocNum, 
	DocDate,
	DocPeriod,
	KolProd,
	OldCenProd,
	ValName = ISNULL(v.ShortName,''),
	SumProd = case when KodCenProd = 'RB' and  Docdate &lt; '20160701' then ROUND(OldCenProd * KolProd, 0) 
	               else ROUND(OldCenProd * KolProd, dbo.uf_valprecis(KodCenProd)) 
	          end,
	NdsStake,
	SumNds = case when KodCenProd = 'RB' and  Docdate &lt; '20160701' 
	                then ROUND(ROUND(OldCenProd * KolProd, 0) * ISNULL(NdsStake, 0) / 100, 0)
	              else ROUND(ROUND(OldCenProd * KolProd, dbo.uf_valprecis(KodCenProd)) * ISNULL(NdsStake, 0) / 100, dbo.uf_valprecis(KodCenProd))
	         end,
	oDopNum = rtrim(od.dopdog), 
	oDopDat = od.dat_dopdog, 
	oDogNum = rtrim(od.osn), 
	oDogDat = od.datd
FROM docs o
LEFT JOIN uv_Product pr ON o.kpr = pr.kpr
INNER JOIN uv_Dog od on o.idolddog = od.iddog
LEFT JOIN uv_Valuta v on o.KodCenProd = v.Kodval</CommandText>
      </Query>
