    <DataSet Name="BonusRows">
      <Query>
        <DataSourceName>DS</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@idsf">
            <Value>=Parameters!IdSf.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>;WITH docs AS 
(
	SELECT 
         g.idprilsf,
         OldCenProd = t.CENA,
         Discount = nd.discount
    FROM dbo.SfProduct g
    INNER JOIN dbo.uv_Post nd on g.KDOG = nd.kdog
    INNER JOIN dbo.SfProductOtgr o ON g.idprilsf = o.idprilsf 
    INNER JOIN dbo.arc_p623 t ON o.idp623 = t .idp623 
    WHERE g.idsf = @idsf
    GROUP BY g.idprilsf, t.CENA, nd.discount
),bonusrows as
(
	SELECT 
                                i.datbuch,
		ProdName = ISNULL(pr.name,''),
		KolProd = g.kolf,
		OldCenProd = o.OldCenProd,
		--NewCenProd = pp.stake,
		KodVal = pp.kodval,
		ValName = ISNULL(v.ShortName,''),
		DogDiscount = isnull(o.discount,0),
		RealDiscount = ROUND((-pp.stake / o.OldCenProd) * 100, 2),	
		BonusSumProd = -pp.summa, 
		NdsStake = ISNULL(pn.stake, 0), 
		BonusNdsSum = -ISNULL(pn.summa, 0),                                                      
		nDopNum = rtrim(nd.dopdog), 
		nDopDat = nd.dat_dopdog, 
		nDogNum = rtrim(nd.osn),
		nDogDat = nd.datd,
		nd.discountDocBase
	FROM docs o
	INNER JOIN SfProduct g on o.idprilsf = g.idprilsf
	INNER JOIN SfHeader i on i.IdSf = g.idsf
	INNER JOIN dbo.SfProductPays pp ON g.idprilsf = pp.idprilsf AND pp.PayType = 1 /* продукт*/ 
	LEFT JOIN dbo.SfProductPays pn ON g.idprilsf = pn.idprilsf AND pn.PayType = 2 /* НДС*/ 
	LEFT JOIN uv_Product pr ON g.kpr = pr.kpr
	INNER JOIN uv_Dog nd on i.iddog = nd.iddog
	LEFT JOIN uv_Valuta v on pp.kodval = v.Kodval
),sumprop AS
(
	SELECT 
		ndsprop = dbo.SummaOfValPhrase(SUM(BonusNdsSum), MAX(Kodval)), 
		itogprop = dbo.SummaOfValPhrase(SUM(BonusSumProd + BonusNdsSum), MAX(Kodval))
	FROM bonusrows
 )
SELECT b.*, sp.*, v.decimals
FROM bonusrows b
left join uv_Valuta v on b.kodval = v.kodval
cross apply sumprop sp</CommandText>
      </Query>
