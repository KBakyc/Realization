      <Query>
        <DataSourceName>DS</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@Poup">
            <Value>=Parameters!Poup.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@PkodStr">
            <Value>=JOIN(Parameters!Pkod.Value,",")</Value>
          </QueryParameter>
          <QueryParameter Name="@realtype">
            <Value>=Parameters!realtype.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@usltype">
            <Value>=Parameters!usltype.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@Pkod">
            <Value>=Parameters!Pkod.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@kodval">
            <Value>=Parameters!Kodval.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>select naipoup = dbo.uf_MakePoupsStr(@Poup, @PkodStr),
	balsch = replace((
select realsch as 'data()'
from (select rn = ROW_NUMBER() over (order by 
case  when @realtype = 1 and s.rectype = 0 then 0
         when @usltype = 0 then 0
         when @usltype = 1 and s.rectype = 1 then 0
         when @usltype = 2 and s.rectype = 0 then 0

                                                   else 1
                                              end),
             realsch
      from (select distinct realsch, rectype
            from sSchet s
            where s.poup = @Poup 
            and s.pkod in (@Pkod)  
            and s.kodvaln = @kodval
            and  s.isActive = 1 and isnull(s.realsch,'') &lt;&gt; '')s
      ) o
where @realtype = 0 and @usltype = 0 or rn = 1
FOR XML PATH('')
              ),' ',', ')</CommandText>
      </Query>
