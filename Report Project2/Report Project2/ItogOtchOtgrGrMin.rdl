    <DataSet Name="kmins">
      <Query>
        <DataSourceName>DS</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@isSelectMin">
            <Value>=Parameters!isSelectMin.Value</Value>
            <rd:UserDefined>true</rd:UserDefined>
          </QueryParameter>
          <QueryParameter Name="@KaType">
            <Value>=Parameters!KaType.Value</Value>
            <rd:UserDefined>true</rd:UserDefined>
          </QueryParameter>
          <QueryParameter Name="@Poup">
            <Value>=Parameters!Poup.Value</Value>
            <rd:UserDefined>true</rd:UserDefined>
          </QueryParameter>
          <QueryParameter Name="@dfrom">
            <Value>=Parameters!dfrom.Value</Value>
            <rd:UserDefined>true</rd:UserDefined>
          </QueryParameter>
          <QueryParameter Name="@dto">
            <Value>=Parameters!dto.Value</Value>
            <rd:UserDefined>true</rd:UserDefined>
          </QueryParameter>
        </QueryParameters>
        <CommandText>if @isSelectMin = 0 
	select 0 as kmin, 'Все министерства' as naimin
else
	select distinct 
		g.kmin, 
		naimin = case when ISNULL(m.kmin,0) = 0 
					  then '[0] Не задано'
					  else '[' + m.kstat + '] ' + rtrim(m.naim)
				 end
	from (select distinct kgr  = case when @KaType = 2 then i.kpok else i.kgr end
		  from uv_ActiveSfHeaders i
			inner join SfProduct s on i.IdSf = s.IdSf	
		  where i.poup in (@Poup)
			and i.sumpltr &gt;= 0
			and s.datgr between @dfrom and @dto
			) k
	inner join uv_GrPol g on k.kgr = g.kgr 
	left join dbo.uv_Sprmin m on g.kmin = m.kmin
	order by naimin</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
