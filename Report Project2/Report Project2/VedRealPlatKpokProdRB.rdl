      <Query>
        <DataSourceName>DS</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@poup">
            <Value>=Parameters!Poup.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>select pkod = 0, naipoup = 'Все'
union
select pkod, naipoup = rtrim(naipoup)
from uv_Pkods
where @poup = 33</CommandText>
      </Query>
