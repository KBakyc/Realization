    <DataSet Name="prods">
      <Query>
        <DataSourceName>DS</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@poup">
            <Value>=Parameters!Poup.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@kodval">
            <Value>=Parameters!Kodval.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@otgrfrom">
            <Value>=Parameters!Date11.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@otgrto">
            <Value>=Parameters!Date22.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@dfrom">
            <Value>=Parameters!Date1.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>select 0 as kpr, 'Все'  as namepr

union

select g.kpr, cast(g.kpr as char(8)) + ' ' + sp.name as namepr
from uv_ActiveSfHeaders i
inner join SfProduct g on i.IdSf = g.IdSf
inner join uv_Product sp on g.kpr = sp.kpr
where i.poup = @poup 
and i.kodval = @kodval
and i.sumpltr &gt;= 0
and g.datgr between @otgrfrom and @otgrto		
and (i.PayStatus &lt;&gt; 2 or i.PayDate &gt;= @dfrom)</CommandText>
      </Query>
      <Fields>
        <Field Name="kpr">
          <DataField>kpr</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="namepr">
          <DataField>namepr</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="kpoks">
      <Query>
        <DataSourceName>DS</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@poup">
            <Value>=Parameters!Poup.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@kodval">
            <Value>=Parameters!Kodval.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@otgrfrom">
            <Value>=Parameters!Date11.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@otgrto">
            <Value>=Parameters!Date22.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@dfrom">
            <Value>=Parameters!Date1.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>select 0 as kpok, 'Все' as namekpok			          

union
			          
select i.kpok, cast(i.kpok as CHAR(6)) + ' ' + sk.name as namekpok
from uv_ActiveSfHeaders i
inner join SfProduct g on i.IdSf = g.IdSf
inner join uv_GrPol sk on i.kpok = sk.kgr
where i.poup = @poup 
and i.kodval = @kodval
and i.sumpltr &gt;= 0
and g.datgr between @otgrfrom and @otgrto		
and (i.PayStatus &lt;&gt; 2 or i.PayDate &gt;= @dfrom)</CommandText>
      </Query>
      <Fields>
        <Field Name="kpok">
          <DataField>kpok</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="namekpok">
          <DataField>namekpok</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
