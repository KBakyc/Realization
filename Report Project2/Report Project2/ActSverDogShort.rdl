    <DataSet Name="agrees">
      <Query>
        <DataSourceName>DS</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@poup">
            <Value>=Parameters!Poup.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@isalldogs">
            <Value>=Parameters!isalldogs.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@kpok">
            <Value>=Parameters!kpok.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@dogstr">
            <Value>=Parameters!dogstr.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@dopstr">
            <Value>=Parameters!dopstr.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>IF @isalldogs = 0
begin

declare @agreements table (idAgreement int,	idPrimaryAgreement int,	NumberOfDocument varchar(100),	DateOfDocument date, idStateType int)
insert @agreements
select idAgreement,	idPrimaryAgreement,	NumberOfDocument, DateOfDocument, idStateType
from uv_Agreements
where idCounteragent = @kpok
and idStateType &lt;&gt; 3              
;with agr as
(
select idAgreement, idPrimaryAgreement, NumberOfDocument, DateOfDocument
from @agreements a
where a.idAgreement = a.idPrimaryAgreement
and idStateType &lt;&gt; 3
and 1 = case when isnull(@dogstr,'') = '' then 1
             when a.NumberOfDocument like '%'+@dogstr+'%' then 1
             else 0
        end
and idPrimaryAgreement in (select distinct idagree from uv_Dog d where d.kpok = @kpok
                                                         and 1 = case when 0 in (@poup) then 1
                                                                      when d.poup in (@poup) then 1
                                                                      else 0
                                                                 end)        
)
select --top 100
	docnum = a.NumberOfDocument, 
	docdate = a.DateOfDocument,
	dopid = d.idAgreement,
	dopnum = '№ ' + rtrim(a.NumberOfDocument) + case when d.idAgreement = a.idAgreement then ' от ' + convert(char(10), a.DateOfDocument,104)
	                                          else ' доп. ' + rtrim(d.NumberOfDocument) + case when d.DateOfDocument is null then ''
	                                                                                           else ' от ' + convert(char(10), d.DateOfDocument,104)
	                                                                                      end
	                                     end,
	dopdate = d.DateOfDocument,
	isdop = case when a.idAgreement = d.idAgreement then 0
	             else 1
	        end
from agr a
left join @agreements d on a.idAgreement = d.idPrimaryAgreement-- and d.idAgreement &lt;&gt; a.idAgreement
and 1 = case when isnull(@dopstr,'') = '' then 1
                      when d.NumberOfDocument like @dopstr + '%' then 1
                      else 0
              end
order by docnum, isdop, dopdate
option(recompile)
end
else
begin
select 
    docnum = '', 
	docdate = null,
	dopid = 0,
	dopnum = 'Все',
	dopdate = null,
	isdop = 0
end</CommandText>
      </Query>
