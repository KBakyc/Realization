        <Field Name="docnum">
          <DataField>docnum</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="docdate">
          <DataField>docdate</DataField>
          <rd:TypeName>System.DateTime</rd:TypeName>
        </Field>
        <Field Name="dopid">
