    <DataSet Name="rekviz">
      <Query>
        <DataSourceName>DS</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@poup">
            <Value>=Parameters!Poup.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>select rs = case when @poup in (10,67,68) 
                   then (select RTRIM(value) from nsd2.dbo.dbf_otprav where varname = 'rasschet3') -- в валюте
                   else (select RTRIM(value) from nsd2.dbo.dbf_otprav where varname = 'rasschet1') -- в рублях РБ
            end,
       bank = case when @poup in (10,67,68) 
                     then (select RTRIM(value) from nsd2.dbo.dbf_otprav where varname = 'otdelgbv1') + ', ' + (select RTRIM(value) from nsd2.dbo.dbf_otprav where varname = 'otdelgbv2')
                     else (select RTRIM(value) from nsd2.dbo.dbf_otprav where varname = 'otdelgosb1') + ', ' + (select RTRIM(value) from nsd2.dbo.dbf_otprav where varname = 'otdelgosb2')
                   end,
       mfo = case when @poup in (10,67,68) 
                    then (select RTRIM(value) from nsd2.dbo.dbf_otprav where varname = 'kodmfo3')
                    else (select RTRIM(value) from nsd2.dbo.dbf_otprav where varname = 'kodmfo1')
             end,
       forwhat = case when @poup = 4 then 'продукцию вспомогательных производств'
                      when @poup = 3 then 'продукцию ТНП'
                      when @poup in(20,22,53,56,60) then 'транспортные услуги'
                      when @poup in (30,33) then 'процессинг'
                      when @poup = 21 then 'возмещение акцизного налога'
                      when @poup = 50 then 'услуги'
                      when @poup = 9 then 'зататок для участия в тендере'
                      when @poup = 73 then 'сырье основного производства'
                      when @poup in (5,10,67,68) then 'нефтепродукты'
                      when @poup = 41 then 'аренду с/х транспортных средств'
                      when @poup = 42 then 'аренду строительных машин'
                      else 'товары/услуги'
                 end</CommandText>
      </Query>
      <Fields>
        <Field Name="rs">
          <DataField>rs</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
