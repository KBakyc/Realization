    <DataSet Name="kpokinfo">
      <Query>
        <DataSourceName>DS</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@kpok">
            <Value>=Parameters!kpok.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>if @kpok  = 0

select kpok = 0,
name = 'Все',
shortname = 'Все',
gorod =  ''

else

select
kpok = kgr,
name = name,
shortname = shortname,
gorod =  Rtrim(gor)
from uv_GrPol
where kgr = @kpok</CommandText>
      </Query>
      <Fields>
