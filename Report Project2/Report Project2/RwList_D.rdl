  <DataSets>
    <DataSet Name="repdata">
      <Query>
        <DataSourceName>DS</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@idrwlist">
            <Value>=Parameters!idrwlist.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>select 
l.num_rwlist,
l.num_inv,
l.dat_inv,
l.bgn_date,
l.end_date,
d.nkrt,
d.num_doc,
d.dat_doc,
d.note,
d.paycode,
t.Payname,
sum_doc = isnull(d.sum_doc,0),
d.ndsrate,
sum_nds = isnull(d.sum_nds,0),
sum_itog = isnull(d.sum_doc,0) + isnull(d.sum_nds,0)
from dbo.RwList l
inner join dbo.RwDoc d on l.id_rwlist = d.id_rwlist
left join dbo.RwPayTypes t on d.paycode = t.Paycode
where l.id_rwlist = @idrwlist</CommandText>
      </Query>
