    <DataSet Name="products">
      <Query>
        <DataSourceName>DS</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@Date1">
            <Value>=Parameters!Date1.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@Date2">
            <Value>=Parameters!Date2.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@Poup">
            <Value>=Parameters!Poup.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>;with kprs as
(
select distinct o.kprod
from uv_Otgruz o
inner join dbo.uv_Post p on o.kdog = p.kdog
inner join dbo.uv_Dog d on p.iddog = d.iddog
where o.datgr between @Date1 and @Date2	
and 1 = case when 0 in (@Poup) then 1
                      when d.poup in (@Poup) then 1
                      else 0
              end
)
select * from (
select 0 as kpr, 'Все' as namepr

union

select kpr, namepr = '['+cast(kpr as char(6))+'] ' + sp.name
from kprs o
inner join dbo.uv_Product sp on o.kprod = sp.kpr
) t
order by kpr
OPTION(RECOMPILE)</CommandText>
      </Query>
