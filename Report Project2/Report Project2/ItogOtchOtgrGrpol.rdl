    <DataSet Name="kpoks">
      <Query>
        <DataSourceName>DS</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@Poup">
            <Value>=Parameters!Poup.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@isSelectKpok">
            <Value>=Parameters!isSelectKpok.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@dfrom">
            <Value>=Parameters!dfrom.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@dto">
            <Value>=Parameters!dto.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>if @isSelectKpok = 0 
select 0 as kpok, 'Все' as namekpok

else
select k.kpok, '[' + cast(k.kpok as CHAR(6)) + '] ' + rtrim(sk.gor) + ' ' + rtrim(sk.name) as namekpok
from (select distinct kpok 
        from uv_ActiveSfHeaders i
        where i.poup in (@Poup)
        and i.sumpltr &gt;= 0
        and i.datbuch between @dfrom and @dto
        ) k
  inner join uv_GrPol sk on k.kpok = sk.kgr 
order by k.kpok</CommandText>
      </Query>
      <Fields>
        <Field Name="kpok">
          <DataField>kpok</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="namekpok">
          <DataField>namekpok</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
