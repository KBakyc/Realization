  <DataSets>
    <DataSet Name="DS">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandText>SELECT
	c.*,
	pvalname = v2.ShortName,
	oldost = c.sumpropl - c.sumotgr,
    newsumotgr = c.sumotgr - c.sumopl,
    newost = c.sumpropl - c.sumotgr + c.sumopl,
    i.numsf,
    i.datpltr,
    i.datbuch,
    i.poup,
    i.kpok,
    i.sumpltr,
    svalname = v1.ShortName,
    kpokname = gp.name
  FROM tmp_LastPredoplsChanges c
  inner join SfHeader i on c.idsf = i.IdSf
  inner join uv_Valuta v1 on i.kodval = v1.Kodval
  inner join uv_Valuta v2 on c.kodval = v2.Kodval
  inner join uv_GrPol gp on i.kpok = gp.kgr
  where c.userid = dbo.uf_GetCurrentUserToken()</CommandText>
      </Query>
