  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>DS</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@idsf">
            <Value>=Parameters!IdSf.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>;WITH prils AS 
(
	SELECT rnum = ROW_NUMBER() OVER (partition BY g.idprilsf ORDER BY t.idrnn),               
           DocNum = CASE WHEN i.SfTypeId = 2 -- служебка 013/226 23.02.15
                           THEN t.RwBillNumber ELSE t.DocumentNumber END,
           DocDate = t.datgr, Kolprod = g.kolf,
           o_ndsstake = t.PRODNDS,
           d.nonds,
           g.idprilsf, g.kpr,
           i.datbuch
    FROM dbo.SfProduct g     
    INNER JOIN dbo.SfProductOtgr o ON g.idprilsf = o.idprilsf 
    INNER JOIN dbo.arc_p623 t ON o.idp623 = t .idp623 
    INNER JOIN dbo.SfHeader i on g.IdSf = i.IdSf
    INNER JOIN dbo.uv_Dog d on i.iddog = d.iddog
    WHERE g.idsf = @idsf
),
pays as
(
	SELECT ProdName = pr.name, 
	       DocNum, DocDate, Kolprod, 
           KodCenProd = pp.kodval, 
           Cenprod = pp.stake,
           SumProd = pp.summa, 
           NdsStake = ISNULL(pn.stake, 0),            
           NdsKodVal = isnull(pn.kodval, ''), 
           NdsSum = ISNULL(pn.summa, 0), 
           o_ndsstake,
           nonds,
           SumItog = pp.summa + ISNULL(pn.summa, 0),
           datbuch
    FROM prils g
    LEFT JOIN uv_Product pr ON g.kpr = pr.kpr 
    INNER JOIN dbo.SfProductPays pp ON g.idprilsf = pp.idprilsf AND pp.PayType = 1 /* продукт*/ 
    LEFT JOIN dbo.SfProductPays pn ON g.idprilsf = pn.idprilsf AND pn.PayType = 2 /* НДС*/ 
    WHERE g.rnum = 1
       
    union all
       
    SELECT ProdName = (select case when i.PayType = 5 then dbo.uf_TunePayName(g.idprilsf, 5) else i.PayName end
                       from (
                          select rn = ROW_NUMBER() over (order by pti.sflinepos),
                                 pti.PayType,
                                 pti.PayName
                          from dbo.SfPayTypes pti
                          where pti.SfLine = pt.sfline) i
                       where i.rn = 1),
           DocNum, DocDate, 
           Kolprod = 0,
           KodCenProd = MAX(case when pt.SfLinePos = 6 then pp.kodval else null end), 
           Cenprod = MAX(case when pt.SfLinePos = 6 then pp.stake else null end),
           SumProd = SUM(case when pt.SfLinePos = 6 then pp.summa else 0 end), 
           NdsStake = MAX(case when pt.SfLinePos = 9 then pp.stake else null end), 
           NdsKodVal = MAX(case when pt.SfLinePos = 9 then pp.kodval else null end), 
           NdsSum = SUM(case when pt.SfLinePos = 9 then pp.summa else 0 end), 
           o_ndsstake,
           nonds,
           SumItog = SUM(case when pt.SfLinePos = 6 then pp.summa else 0 end) + SUM(case when pt.SfLinePos = 9 then pp.summa else 0 end),
           datbuch
    FROM prils g
    INNER JOIN dbo.SfProductPays pp ON g.idprilsf = pp.idprilsf AND pp.PayType &gt; 2 /*доп сборы*/ 
    inner join dbo.SfPayTypes pt on pp.PayType = pt.PayType and pt.SfLine &gt; 1    
    WHERE g.rnum = 1   
    group by g.idprilsf, pt.SfLine, DocNum, DocDate, o_ndsstake, nonds, datbuch
), 
sumprop AS
(SELECT
     ndsprop = dbo.SummaOfValPhrase(SUM(ndssum), MAX(NdsKodval)), 
     itogprop = dbo.SummaOfValPhrase(SUM(sumitog), MAX(Kodcenprod))
 FROM pays
)
SELECT  ProdName, DocNum, DocDate, KolProd, CenProd, KodCenProd, SumProd, NdsStake, NdsSum, SumItog, 
        zerondsfmt = case when o_ndsstake = -1 or nonds = 2 then '---' else '0' end,
        ndsprop = case when NdsSum &lt; 0 then 'Минус ' + ndsprop else ndsprop end, 
        itogprop = case when SumItog &lt; 0 then 'Минус ' + itogprop else itogprop end
FROM pays 
CROSS JOIN sumprop</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
