          <QueryParameter Name="@IdSf">
            <Value>=Parameters!IdSf.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>;with datotgr as
(
select top 1 g.datgr
from dbo.SfProduct g
where g.idsf = @IdSf
)
select 
	period = dbo.[uf_DatMake]((select datgr from datotgr),2),
	prod_name = 'отгруженных товаров(возмещаемых услуг)',
	--prod_name = case when i.poup = 73 then 'отгруженной нефти сырой'
	--				else 'отгруженных нефтепродуктов' end,
	ord_dat = '19.01.2015',
	ord_num = '97',	
	--ord_dat = case when i.poup = 73 then '26 апреля 2010 г.'
	--				else '27 февраля 2009 г.' end,
	--ord_num = case when i.poup = 73 then '491'
	--				else '240' end,
	ot.ourname as OurName,
	OurAddress = rtrim(ot.ouraddr) + ', ' + rtrim(ot.ourcountryname) 
	             + ', р/с ' + rtrim(ob.rsh) + ' в ' + ob.bank + ', МФО ' + ob.mfo,
	OurUnp = ot.ourunp,
	i.numsf,
	i.datpltr,
	valname = v.ShortName,
	d.DatOsn,
	DopOsn = isnull(dbo.uf_GetSfDopDogStr(@IdSf),d.DopOsn),
	d.NaiOsn,
	d.OsnTxt,
	d.idagree,
	d.iddog,
	d.idporsh,
	d.srok,
	d.typrespite,
	Name = gr.fullname,
	gr.[address],
	gr.inn,
	sgn.*,
	i.SfStatus,
	d.nonds
from dbo.SfHeader i
--inner join dbo.uv_Dog d on i.iddog = d.iddog
left join dbo.uv_Valuta v on i.kodval = v.Kodval
cross apply dbo.uf_GetOurBankInfo(i.iddog) ob
cross apply dbo.uf_GetDogInfo(i.iddog) d
cross join dbo.uv_Otprav ot
cross apply dbo.uf_GetKontragentInfo(i.kpok) gr
outer apply dbo.uf_GetSigns(i.poup) sgn
where i.IdSf = @IdSf</CommandText>
