      <Query>
        <DataSourceName>DS</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@Poup">
            <Value>=Parameters!Poup.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@Pkod">
            <Value>=Parameters!Pkod.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@Kodval">
            <Value>=Parameters!Kodval.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>select naipoup,
	naipkod = case when @Pkod &lt;&gt; 0 
	                 then (select pk.naipoup from uv_Pkods pk where pk.pkod = @Pkod)
	          end,
	balsch = (
	          select top 1 kre = left(kre,6)
              from sSchet s
              where s.poup = @Poup and s.pkod = @Pkod 
              and  s.isActive = 1 and isnull(s.kre,'') &lt;&gt; ''
              and s.kodvaln = @Kodval
              )
from uv_Moup
where kodpoup = @Poup</CommandText>
      </Query>
