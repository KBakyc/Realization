    <DataSet Name="poups">
      <Query>
        <DataSourceName>DS</DataSourceName>
        <CommandText>select kodpoup, naipoup = '[' + cast(kodpoup as varchar(10)) + '] ' + rtrim(naipoup)
from dbo.uf_GetMyActivePoups()</CommandText>
      </Query>
      <Fields>
        <Field Name="kodpoup">
          <DataField>kodpoup</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="naipoup">
          <DataField>naipoup</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="products">
      <Query>
        <DataSourceName>DS</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@Poup">
            <Value>=Parameters!Poup.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@ondate">
            <Value>=Parameters!ondate.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>select kpr, nameprod = cast(kpr as char(6)) + ' ' +  name
from uv_Product
where 
kpr in 
(
select distinct g.kpr
from SfHeader i
inner join SfProduct g on i.IdSf = g.IdSf
where i.SfStatus not in (1,5)
and i.poup = @Poup
and i.datbuch &lt; @ondate
and (i.PayStatus &lt;&gt; 2 or i.PayDate &gt;=@ondate)
and i.sumpltr &gt; 0
)</CommandText>
      </Query>
      <Fields>
        <Field Name="kpr">
          <DataField>kpr</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="nameprod">
          <DataField>nameprod</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
