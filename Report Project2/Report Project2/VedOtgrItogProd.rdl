    <DataSet Name="Prods">
      <Query>
        <DataSourceName>DS</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@Pkod">
            <Value>=Parameters!Pkod.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@Poup">
            <Value>=Parameters!Poup.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@uslType">
            <Value>=Parameters!uslType.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@dfrom">
            <Value>=Parameters!dfrom.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@dto">
            <Value>=Parameters!dto.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@isselectprod">
            <Value>=Parameters!isSelectProd.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>if @isselectprod = 0
select distinct
  kpr = 0,
  nameprod = 'Все'

else

select distinct
  g.kpr,
  nameprod = cast(g.kpr as char(8)) + ' ' + rtrim(sp.name)
from uv_ActiveSfHeaders i
inner join SfProduct g on i.IdSf = g.IdSf
inner join uv_Product sp on g.kpr = sp.kpr
		and 1 = case when 0 in (@Pkod) then 1
			 when sp.pkod in (@Pkod) then 1
			 else 0
			end
       and 1 = case when @uslType = 0 then 1
                              when @uslType = 1 and sp.inrealiz = 0 then 1
                              when @uslType = 2 and sp.inrealiz = 1 then 1  
                              else 0
                      end
where i.poup in (@Poup)
and g.datgr between @dfrom and @dto
order by g.kpr</CommandText>
      </Query>
      <Fields>
        <Field Name="kpr">
          <DataField>kpr</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="nameprod">
          <DataField>nameprod</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
