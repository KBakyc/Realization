    <DataSet Name="pkods">
      <Query>
        <DataSourceName>DS</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@Poup">
            <Value>=Parameters!Poup.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>if 33 in (@Poup)
select pkod = 0, naipoup = 'Все'
union
select pkod, naipoup = rtrim(naipoup)
from uv_Pkods
else
select pkod = 0, naipoup = 'Все'</CommandText>
      </Query>
      <Fields>
